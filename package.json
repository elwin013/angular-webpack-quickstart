{
    "name": "angular-webpack-quickstart",
    "version": "0.2.0",
    "description": "Simple starter for AngularJS and Bootstrap using Webpack.",
    "scripts": {
        "build": "rimraf dist && webpack --config webpack.config.js --progress --profile --bail",
        "server": "webpack-dev-server --history-api-fallback --inline --content-base dist/",
        "start": "npm run server"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/elwin013/angular-webpack-quickstart.git"
    },
    "author": "Kamil \"elwin013\" Banach",
