    "homepage": "https://github.com/elwin013/angular-webpack-quickstart",
    "devDependencies": {
        "babel-core": "^6.18.2",
        "babel-loader": "^6.2.7",
        "babel-preset-es2015": "^6.18.0",
        "copy-webpack-plugin": "^4.0.0",
        "css-loader": "^0.25.0",
        "file-loader": "^0.9.0",
        "html-loader": "^0.4.4",
        "raw-loader": "^0.5.1",
        "rimraf": "^2.5.4",
        "style-loader": "^0.13.1",
        "url-loader": "^0.5.7",
        "webpack": "^1.13.3",
        "webpack-dev-server": "^1.16.2"
    },
    "dependencies": {
